@model Cloud_based_editor_VLN_2.Models.Entities.Project

    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Add project </h4>
            </div>
            <div class="modal-body" id="InviteUserModal">
                <form id="AddProjectForm">

                    @Html.HiddenFor(model => model.OwnerID)
                    @Html.LabelFor(model => model.Name)
                    @Html.TextBoxFor(model => model.Name, new {@id="AddProjectTextBox" })
                    @*@Html.TextBox("InviteTxtBox", "", new { placeholder = "Please enter username" })*@
                    <div class="text-danger" id="AddProjectErrorDiv">

                    </div>
                    
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" onclick="AddnewProjectFunc()" id="InviteUserSubmitBtn">Create Project</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </form>
            </div>
        </div>  
        </div>

